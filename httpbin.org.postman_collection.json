		{
			"name": "POST with URL Params",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json();",
							"",
							"pm.test(\"name check\", function () {",
							"    pm.expect(jsonData.json.name).to.eql(pm.globals.get(\"name\"))});",
							"",
							"pm.test(\"email check\", function () {",
							"    pm.expect(jsonData.json.email).to.eql(pm.globals.get(\"email\"))});",
							"",
							"pm.test(\"permissions check\", function () {",
							"    pm.expect(jsonData.json.permissions).to.include(4000)});",
							"",
							"",
							"console.log(jsonData.json.permissions[2]);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"pm.globals.set(\"name\", \"Tim\");",
							"pm.globals.set(\"email\", \"tim.chapman23423423@gmail.com\")",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"{{name}}\",\n    \"permissions\": [2000,3000,3001,4000],\n    \"email\": \"{{email}}\"\n}",
					"options": {
